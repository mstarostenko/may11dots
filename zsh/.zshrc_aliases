# go to orgs
alias cdo="cd ~/Documents/may11docs"
# go to dotfiles
alias cdd="cd ~/.dotfiles"
# go to project folder
alias cdp="cd ~/projects"
# go to project Downloads
alias cdD="cd ~/Downloads"
# go to project Trash
alias cdT="cd ~/.local/share/Trash"



